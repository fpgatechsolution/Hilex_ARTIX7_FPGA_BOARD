  create_bd_pin -dir I -from 0 -to 0 In0
  create_bd_pin -dir I dcm_locked
  create_bd_pin -dir I -type rst reset
  create_bd_pin -dir O -from 0 -to 0 -type rst s_axi_aresetn
  create_bd_pin -dir I -type clk sys_clk_i

