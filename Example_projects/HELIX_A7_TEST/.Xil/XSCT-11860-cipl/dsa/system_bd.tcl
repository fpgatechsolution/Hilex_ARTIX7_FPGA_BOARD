   CONFIG.MMCM_DIVCLK_DIVIDE {1} \
   CONFIG.NUM_OUT_CLKS {5} \
   CONFIG.RESET_BOARD_INTERFACE {reset} \
   CONFIG.RESET_PORT {resetn} \
   CONFIG.RESET_TYPE {ACTIVE_LOW} \
   CONFIG.USE_BOARD_FLOW {true} \
 ] $clk_wiz_0

  # Create instance: hdmi_subsystem
  create_hier_cell_hdmi_subsystem [current_bd_instance .] hdmi_subsystem

  # Create instance: microblaze_subsystem
  create_hier_cell_microblaze_subsystem [current_bd_instance .] microblaze_subsystem

  # Create instance: vga_decimate_0, and set properties
  set block_name vga_decimate
  set block_cell_name vga_decimate_0
  if { [catch {set vga_decimate_0 [create_bd_cell -type module -reference $block_name $block_cell_name] } errmsg] } {
     catch {common::send_msg_id "BD_TCL-105" "ERROR" "Unable to add referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
