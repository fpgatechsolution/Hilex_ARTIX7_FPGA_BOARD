# Hierarchical cell: hdmi_subsystem
proc create_hier_cell_hdmi_subsystem { parentCell nameHier } {

  variable script_folder

  if { $parentCell eq "" || $nameHier eq "" } {
     catch {common::send_msg_id "BD_TCL-102" "ERROR" "create_hier_cell_hdmi_subsystem() - Empty argument(s)!"}
     return
  }

  # Get object for parentCell
  set parentObj [get_bd_cells $parentCell]
  if { $parentObj == "" } {
     catch {common::send_msg_id "BD_TCL-100" "ERROR" "Unable to find parent cell <$parentCell>!"}
     return
  }

  # Make sure parentObj is hier blk
  set parentType [get_property TYPE $parentObj]
  if { $parentType ne "hier" } {
     catch {common::send_msg_id "BD_TCL-101" "ERROR" "Parent <$parentObj> has TYPE = <$parentType>. Expected to be <hier>."}
     return
  }

  # Save current instance; Restore later
  set oldCurInst [current_bd_instance .]

  # Set parent object as current
  current_bd_instance $parentObj

  # Create cell and set as current instance
  set hier_obj [create_bd_cell -type hier $nameHier]
  current_bd_instance $hier_obj

  # Create interface pins

  # Create pins
  create_bd_pin -dir I clk_pix_1x
  create_bd_pin -dir I clk_pix_5x
  create_bd_pin -dir I clk_pix_5x_180
  create_bd_pin -dir I -from 0 -to 0 de
  create_bd_pin -dir I -from 23 -to 0 din
  create_bd_pin -dir I hsync
  create_bd_pin -dir I -type rst rst
  create_bd_pin -dir O -from 3 -to 0 tmds
  create_bd_pin -dir O -from 3 -to 0 tmdsb
  create_bd_pin -dir I vsync

  # Create instance: blue8, and set properties
  set blue8 [ create_bd_cell -type ip -vlnv xilinx.com:ip:xlslice:1.0 blue8 ]
  set_property -dict [ list \
   CONFIG.DIN_FROM {7} \
   CONFIG.DIN_TO {0} \
   CONFIG.DIN_WIDTH {24} \
   CONFIG.DOUT_WIDTH {8} \
 ] $blue8

  # Create instance: dvi_top_0, and set properties
  set block_name dvi_top
  set block_cell_name dvi_top_0
  if { [catch {set dvi_top_0 [create_bd_cell -type module -reference $block_name $block_cell_name] } errmsg] } {
     catch {common::send_msg_id "BD_TCL-105" "ERROR" "Unable to add referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   } elseif { $dvi_top_0 eq "" } {
     catch {common::send_msg_id "BD_TCL-106" "ERROR" "Unable to referenced block <$block_name>. Please add the files for ${block_name}'s definition into the project."}
     return 1
   }
  
  # Create instance: green8, and set properties
  set green8 [ create_bd_cell -type ip -vlnv xilinx.com:ip:xlslice:1.0 green8 ]
  set_property -dict [ list \
   CONFIG.DIN_FROM {15} \
   CONFIG.DIN_TO {8} \
   CONFIG.DIN_WIDTH {24} \
   CONFIG.DOUT_WIDTH {8} \
 ] $green8

  # Create instance: red8, and set properties
  set red8 [ create_bd_cell -type ip -vlnv xilinx.com:ip:xlslice:1.0 red8 ]
  set_property -dict [ list \
   CONFIG.DIN_FROM {23} \
   CONFIG.DIN_TO {16} \
   CONFIG.DIN_WIDTH {24} \
   CONFIG.DOUT_WIDTH {8} \
 ] $red8

  # Create instance: util_vector_logic_0, and set properties
  set util_vector_logic_0 [ create_bd_cell -type ip -vlnv xilinx.com:ip:util_vector_logic:2.0 util_vector_logic_0 ]
  set_property -dict [ list \
   CONFIG.C_OPERATION {not} \
   CONFIG.C_SIZE {1} \
   CONFIG.LOGO_FILE {data/sym_notgate.png} \
 ] $util_vector_logic_0

  # Create port connections
  connect_bd_net -net Op1_1 [get_bd_pins de] [get_bd_pins dvi_top_0/de] [get_bd_pins util_vector_logic_0/Op1]
  connect_bd_net -net blue8_Dout [get_bd_pins blue8/Dout] [get_bd_pins dvi_top_0/blue]
  connect_bd_net -net clk_wiz_0_clk40 [get_bd_pins clk_pix_1x] [get_bd_pins dvi_top_0/clk_pix_1x]
  connect_bd_net -net clk_wiz_0_clk_pix_5x [get_bd_pins clk_pix_5x] [get_bd_pins dvi_top_0/clk_pix_5x]
  connect_bd_net -net clk_wiz_0_clk_pix_5x_180 [get_bd_pins clk_pix_5x_180] [get_bd_pins dvi_top_0/clk_pix_5x_180]
  connect_bd_net -net dvi_top_0_tmds [get_bd_pins tmds] [get_bd_pins dvi_top_0/tmds]
  connect_bd_net -net dvi_top_0_tmdsb [get_bd_pins tmdsb] [get_bd_pins dvi_top_0/tmdsb]
  connect_bd_net -net green8_Dout [get_bd_pins dvi_top_0/green] [get_bd_pins green8/Dout]
  connect_bd_net -net red8_Dout [get_bd_pins dvi_top_0/red] [get_bd_pins red8/Dout]
  connect_bd_net -net rst_1 [get_bd_pins rst] [get_bd_pins dvi_top_0/rst]
  connect_bd_net -net util_vector_logic_0_Res [get_bd_pins dvi_top_0/blank] [get_bd_pins util_vector_logic_0/Res]
  connect_bd_net -net v_axi4s_vid_out_0_vid_data [get_bd_pins din] [get_bd_pins blue8/Din] [get_bd_pins green8/Din] [get_bd_pins red8/Din]
  connect_bd_net -net v_axi4s_vid_out_0_vid_hsync [get_bd_pins hsync] [get_bd_pins dvi_top_0/hsync]
  connect_bd_net -net v_axi4s_vid_out_0_vid_vsync [get_bd_pins vsync] [get_bd_pins dvi_top_0/vsync]

  # Restore current instance
  current_bd_instance $oldCurInst
}


# Procedure to create entire design; Provide argument to make
# procedure reusable. If parentCell is "", will use root.
proc create_root_design { parentCell } {

  variable script_folder
  variable design_name

  if { $parentCell eq "" } {
     set parentCell [get_bd_cells /]
  }

  # Get object for parentCell
  set parentObj [get_bd_cells $parentCell]
  if { $parentObj == "" } {
     catch {common::send_msg_id "BD_TCL-100" "ERROR" "Unable to find parent cell <$parentCell>!"}
     return
  }

  # Make sure parentObj is hier blk
  set parentType [get_property TYPE $parentObj]
  if { $parentType ne "hier" } {
     catch {common::send_msg_id "BD_TCL-101" "ERROR" "Parent <$parentObj> has TYPE = <$parentType>. Expected to be <hier>."}
     return
  }

  # Save current instance; Restore later
  set oldCurInst [current_bd_instance .]

  # Set parent object as current
  current_bd_instance $parentObj


  # Create interface ports
  set ddr3_sdram [ create_bd_intf_port -mode Master -vlnv xilinx.com:interface:ddrx_rtl:1.0 ddr3_sdram ]
  set usb_uart [ create_bd_intf_port -mode Master -vlnv xilinx.com:interface:uart_rtl:1.0 usb_uart ]

  # Create ports
  set blue [ create_bd_port -dir O -from 1 -to 0 blue ]
  set green [ create_bd_port -dir O -from 2 -to 0 green ]
  set hsync [ create_bd_port -dir O hsync ]
  set red [ create_bd_port -dir O -from 2 -to 0 red ]
  set reset [ create_bd_port -dir I -type rst reset ]
  set_property -dict [ list \
   CONFIG.POLARITY {ACTIVE_LOW} \
 ] $reset
  set sys_clock [ create_bd_port -dir I -type clk sys_clock ]
  set_property -dict [ list \
   CONFIG.FREQ_HZ {100000000} \
   CONFIG.PHASE {0.000} \
 ] $sys_clock
  set tmds [ create_bd_port -dir O -from 3 -to 0 tmds ]
  set tmdsb [ create_bd_port -dir O -from 3 -to 0 tmdsb ]
  set vsync [ create_bd_port -dir O vsync ]

  # Create instance: clk_wiz_0, and set properties
  set clk_wiz_0 [ create_bd_cell -type ip -vlnv xilinx.com:ip:clk_wiz:5.4 clk_wiz_0 ]
  set_property -dict [ list \
   CONFIG.CLKOUT2_JITTER {114.829} \
   CONFIG.CLKOUT2_PHASE_ERROR {98.575} \
   CONFIG.CLKOUT2_REQUESTED_OUT_FREQ {200.000} \
   CONFIG.CLKOUT2_USED {true} \
   CONFIG.CLKOUT3_JITTER {159.371} \
   CONFIG.CLKOUT3_PHASE_ERROR {98.575} \
   CONFIG.CLKOUT3_REQUESTED_OUT_FREQ {40.000} \
   CONFIG.CLKOUT3_USED {true} \
   CONFIG.CLKOUT4_JITTER {114.829} \
   CONFIG.CLKOUT4_PHASE_ERROR {98.575} \
   CONFIG.CLKOUT4_REQUESTED_OUT_FREQ {200.000} \
   CONFIG.CLKOUT4_USED {true} \
   CONFIG.CLKOUT5_JITTER {114.829} \
   CONFIG.CLKOUT5_PHASE_ERROR {98.575} \
   CONFIG.CLKOUT5_REQUESTED_OUT_FREQ {200.000} \
   CONFIG.CLKOUT5_REQUESTED_PHASE {180.000} \
   CONFIG.CLKOUT5_USED {true} \
